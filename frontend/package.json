{
  "name": "conversor-divisas",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "axios": "^1.6.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-router-dom": "^6.20.0",
    "react-scripts": "5.0.1",
    "recharts": "^2.10.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "postbuild": "react-snap",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.32",
    "react-snap": "^1.23.0",
    "tailwindcss": "^3.3.5"
  },
  "reactSnap": {
    "puppeteerArgs": [
      "--no-sandbox",
      "--disable-setuid-sandbox"
    ],
    "include": [
      "/",
      "/about",
      "/blog",
      "/faq",
      "/blog/como-evitar-comisiones-ocultas-2025",
      "/blog/mejor-momento-cambiar-euros-dolares",
      "/dolar-a-peso-argentino",
      "/blog/mejores-apps-cambio-moneda"
    ],
    "head": [
      [
        "link",
        {
          "rel": "canonical",
          "href": "https://convertidordivisas.com{{pathname}}"
        }
      ]
    ]
  }
 }
